  "java.format.settings.profile": "GoogleStyle",
  "java.errors.incompleteClasspath.severity": "warning",
  "java.configuration.maven.userSettings": "${workspaceFolder}/.mvn/settings.xml",
  "files.associations": {
    "*.properties": "properties",
    "*.yml": "yaml"
